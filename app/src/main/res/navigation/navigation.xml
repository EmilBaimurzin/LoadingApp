<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@id/fragmentLoading">

    <fragment
        android:id="@+id/fragmentLoading"
        android:name="com.loading.application.ui.loading.FragmentLoading"
        android:label="FragmentLoading" >
        <action
            android:id="@+id/action_fragmentLoading_to_fragmentMain"
            app:destination="@id/fragmentMain" />
    </fragment>
    <fragment
        android:id="@+id/fragmentMain"
        android:name="com.loading.application.ui.main.FragmentMain"
        android:label="FragmentMain" />
</navigation>